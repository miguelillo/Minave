services:
  minave.api:
    build:
      context: C:\Projects\Squad For Fun\Minave
      dockerfile: Minave.API/Dockerfile
    image: minaveapi
    ports:
    - published: 1500
      target: 5000
    restart: always
  minave.app:
    build:
      context: C:\Projects\Squad For Fun\Minave\Minave.APP
      dockerfile: Dockerfile
